{
    "name": "auth-service",
    "version": "1.0.0",
    "description": "The user managment service",
    "main": "index.js",
    "scripts": {
        "dev": "cross-env NODE_ENV=dev nodemon --legacy-watch --exec ts-node src/server.ts",
        "build": "tsc",
        "format:fix": "prettier . --write",
        "format:check": "prettier . --check",
        "lint:check": "eslint .",
        "lint:fix": "eslint . --fix",
        "test": "jest --watch --runInBand",
        "prepare": "husky",
        "start": "ts-node src/index.ts",
        "migration:generate": "typeorm-ts-node-commonjs migration:generate", 
        "migration:run": "typeorm-ts-node-commonjs migration:run",
        "migration:create": "typeorm-ts-node-commonjs migration:create"
    },
    "author": "Rekhta <rikhtamenahil@gmail.com>",
    "license": "ISC",
    "devDependencies": {
        "@eslint/js": "^9.29.0",
        "@types/bcrypt": "^5.0.2",
        "@types/cookie-parser": "^1.4.9",
        "@types/express": "^5.0.3",
        "@types/http-errors": "^2.0.5",
        "@types/jest": "^30.0.0",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/node": "^24.0.3",
        "@types/supertest": "^6.0.3",
        "@types/winston": "^2.4.4",
        "cross-env": "^7.0.3",
        "eslint": "^9.29.0",
        "husky": "^9.1.7",
        "jest": "^30.0.2",
        "lint-staged": "^16.1.2",
        "mock-jwks": "^1.0.10",
        "nodemon": "^3.1.10",
        "prettier": "3.5.3",
        "supertest": "^7.1.1",
        "ts-jest": "^29.4.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.34.1"
    },
    "lint-staged": {
        "*.ts": [
            "npm run format:fix",
            "npm run lint:fix"
        ]
    },
    "dependencies": {
        "bcrypt": "^6.0.0",
        "cookie-parser": "^1.4.7",
        "dotenv": "^16.5.0",
        "express": "^5.1.0",
        "express-jwt": "^8.5.1",
        "express-validator": "^6.15.0",
        "http-errors": "^2.0.0",
        "jsonwebtoken": "^9.0.2",
        "jwks-rsa": "^3.2.0",
        "pg": "^8.14.1",
        "reflect-metadata": "^0.2.2",
        "rsa-pem-to-jwk": "^1.1.3",
        "typeorm": "0.3.25",
        "winston": "^3.17.0"
    }
}
